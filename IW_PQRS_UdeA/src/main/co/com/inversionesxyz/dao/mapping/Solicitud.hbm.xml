<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="co.com.inversionesxyz.dto.Solicitud" table="SOLICITUD" catalog="iwpqrs">
        <id name="id" type="string">
            <column name="id" length="100" />
            <generator class="assigned" />
        </id>
        <many-to-one name="producto" class="co.com.inversionesxyz.dto.Producto" fetch="select">
            <column name="PRODUCTO_codigo" length="100" not-null="true" />
        </many-to-one>
        <many-to-one name="informacionAnalista" class="co.com.inversionesxyz.dto.InformacionAnalista" fetch="select">
            <column name="INFORMACION_ANALISTA_dni" length="20" not-null="true" />
        </many-to-one>
        <property name="tipo" type="string">
            <column name="tipo" length="50" not-null="true" />
        </property>
        <property name="descripcion" type="string">
            <column name="descripcion" length="300" not-null="true" />
        </property>
        <property name="estado" type="string">
            <column name="estado" length="10" not-null="true" />
        </property>
        <property name="complejidad" type="string">
            <column name="complejidad" length="10" />
        </property>
        <property name="prioridad" type="string">
            <column name="prioridad" length="10" />
        </property>
        <property name="fechaCreacion" type="java.util.Date">
            <column name="fecha_creacion" length="19" not-null="true" />
        </property>
        <property name="fechaAtencion" type="java.util.Date">
            <column name="fecha_atencion" length="19" />
        </property>
        <property name="tipoDocumento" type="string">
            <column name="tipo_documento" length="100" not-null="true" />
        </property>
        <property name="nombreCliente" type="string">
            <column name="nombre_cliente" length="45" not-null="true" />
        </property>
    </class>
</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="Solicitud" table="SOLICITUD" catalog="iwpqrs">
    	<meta attribute="class-description">
         	Esta clase contiene el detalle de la solicitud.
      	</meta>
      	
        <id name="id" type="string">
            <column name="id" length="100" />
            <generator class="assigned" />
        </id>
        
        <many-to-one name="producto" class="Producto" fetch="select">
            <column name="PRODUCTO_codigo" length="100" not-null="true" />
        </many-to-one>
        <many-to-one name="analista" class="Analista" fetch="select">
            <column name="ANALISTA_codigo" length="20" not-null="true" />
        </many-to-one>
        <property name="tipo" type="string">
            <column name="tipo" length="50" not-null="true" />
        </property>
        <property name="descripcion" type="string">
            <column name="descripcion" length="300" not-null="true" />
        </property>
        <property name="estado" type="string">
            <column name="estado" length="10" not-null="true" />
        </property>
        <property name="complejidad" type="string">
            <column name="complejidad" length="10" />
        </property>
        <property name="prioridad" type="string">
            <column name="prioridad" length="10" />
        </property>
        <property name="fechaCreacion" type="time">
            <column name="fecha_creacion" length="8" not-null="true" />
        </property>
        <property name="fechaAtencion" type="time">
            <column name="fecha_atencion" length="8" />
        </property>
        <property name="nombreCliente" type="string">
            <column name="nombre_cliente" length="100" />
        </property>
        <property name="correoCliente" type="string">
            <column name="correo_cliente" length="50" />
        </property>
        
    </class>
</hibernate-mapping>
